{
    "manifest_version": 2,
    "name": "Meaning Finder",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "Content_Script/content.js"
            ]
        }
    ],
    "background":{
      "scripts":["BackGround_Script/background.js"]
    },
    "browser_action":{
      "default_icon": "Extension_Icon/search.png",
      "default_popup": "Popup/Popup.html",
      "default_title": "Meaning Finder"
    }
}